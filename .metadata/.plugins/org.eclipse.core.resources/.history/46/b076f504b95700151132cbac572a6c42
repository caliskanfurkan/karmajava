import java.util.Arrays;
import java.util.Collections;
import java.util.List;

public class Test {
	public static void main(String[] args) {
		List<String> list = Arrays.asList("furkan", "ali", "cevdet", "zehra");

		Collections.sort(list, (arg0, arg1) -> arg0.compareTo(arg1) ? false : true);

		for (String obj : list) {
			System.out.println(obj.toString());
		}
	}
}
